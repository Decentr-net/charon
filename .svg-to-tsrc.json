{
  "generateType": "false",
  "delimiter": "KEBAB",
  "conversionType": "files",
  "iconsFolderName": "svg",
  "interfaceName": "SvgIcon",
  "typeName": "SvgIconName",
  "modelFileName": "svg-icon.model",
  "prefix": "svg",
