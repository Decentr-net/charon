{
  "name": "charon",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "cd:charon": "cd projects/charon",
    "cd:chrome": "cd projects/chrome",
    "build": "npm run clean && npm run build:charon && npm run build:chrome",
    "build:prod": "npm run clean && npm run build:charon:prod && npm run build:chrome:prod && npm run pack",
    "build:charon": "ng build charon",
    "build:charon:prod": "ng build charon --prod",
    "build:chrome": "webpack --config projects/chrome/webpack.config.dev.js",
    "build:chrome:prod": "webpack -p --config projects/chrome/webpack.config.prod.js",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "pack": "cd dist && bestzip ../extension-build.zip *",
    "test": "ng test",
    "watch": "npm run clean && npm-run-all --parallel watch:*",
    "watch:charon": "ng build charon --watch --delete-output-path=false --sourceMap=false",
    "watch:chrome": "webpack --config projects/chrome/webpack.config.dev.js -w"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@ngneat/reactive-forms": "^1.4.3",
    "@ngneat/transloco": "^2.19.1",
    "@ngneat/until-destroy": "^8.0.3",
    "d3": "~6.0.0",
    "decentr-js": "^1.2.1",
    "http-status-codes": "^2.1.4",
    "js-sha256": "^0.9.0",
    "ng-inline-svg": "^11.0.0",
    "ngx-toastr": "~12.1.0",
    "p-queue": "^6.6.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "bestzip": "^2.1.7",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.11",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webextension-polyfill-ts": "^0.21.0",
    "webpack-cli": "^3.3.12"
  }
}
