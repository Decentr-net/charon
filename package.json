{
  "name": "charon",
  "version": "3.4.0",
  "scripts": {
    "ng": "ng",
    "analyze:charon": "webpack-bundle-analyzer dist/charon/stats.json",
    "build:chrome": "npm run clean && npm run build:charon && npm run build:scripts:chrome",
    "build:chrome:prod": "npm run clean && npm run build:charon:prod && npm run build:scripts:chrome:prod && cross-env BROWSER=chrome npm run pack",
    "build:firefox": "npm run clean && npm run build:charon && npm run build:scripts:firefox",
    "build:firefox:prod": "npm run clean && npm run build:charon:prod && npm run build:scripts:firefox:prod && cross-env BROWSER=firefox npm run pack",
    "build:all:prod": "npm run clean && npm run build:charon:prod && npm run build:scripts:chrome:prod && cross-env BROWSER=chrome npm run pack_named && npm run build:scripts:firefox:prod && cross-env BROWSER=firefox npm run pack_named",
    "build:charon": "ng build charon",
    "build:charon:prod": "ng build charon --configuration production",
    "build:charon:stats": "ng build charon --stats-json",
    "build:scripts": "webpack --config projects/scripts/webpack.config.dev.js",
    "build:scripts:prod": "webpack --config projects/scripts/webpack.config.prod.js",
    "build:scripts:chrome": "cross-env BROWSER=chrome npm run build:scripts",
    "build:scripts:chrome:prod": "cross-env BROWSER=chrome npm run build:scripts:prod",
    "build:scripts:firefox": "cross-env BROWSER=firefox npm run build:scripts",
    "build:scripts:firefox:prod": "cross-env BROWSER=firefox npm run build:scripts:prod",
    "generate-icons": "svg-generator",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "pack": "cd dist && bestzip ../charon_extension.zip *",
    "pack_named": "cd dist && bestzip ../charon_$(date +'%Y%m%d-%H%M')_v$(node -p -e \"require('../package.json').version\")_$(echo $BROWSER).zip *",
    "test": "ng test",
    "watch:chrome": "npm run clean && npm-run-all --parallel watch:charon watch:scripts:chrome",
    "watch:firefox": "npm run clean && npm-run-all --parallel watch:charon watch:scripts:firefox",
    "watch:charon": "ng build charon --watch --delete-output-path=false --sourceMap=false",
    "watch:scripts": "webpack --config projects/scripts/webpack.config.dev.js -w",
    "watch:scripts:chrome": "cross-env BROWSER=chrome npm run watch:scripts",
    "watch:scripts:firefox": "cross-env BROWSER=firefox npm run watch:scripts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.11",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "~12.2.11",
    "@angular/compiler": "~12.2.11",
    "@angular/core": "~12.2.11",
    "@angular/forms": "~12.2.11",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "~12.2.11",
    "@angular/platform-browser-dynamic": "~12.2.11",
    "@angular/router": "~12.2.11",
    "@ngneat/input-mask": "^3.0.3",
    "@ngneat/reactive-forms": "^3.1.2",
    "@ngneat/svg-icon": "^3.2.0",
    "@ngneat/transloco": "^3.0.6",
    "@ngneat/until-destroy": "^8.1.4",
    "@rxweb/reactive-form-validators": "^2.1.4",
    "clamp-js": "^0.7.0",
    "d3": "~6.0.0",
    "decentr-js": "^2.7.2",
    "highcharts": "^9.3.0",
    "highcharts-angular": "^2.10.0",
    "html2canvas": "1.3.2",
    "http-status-codes": "^2.1.4",
    "inputmask": "^5.0.6",
    "jspdf": "^2.4.0",
    "ngx-skeleton-loader": "^4.0.0",
    "ngx-toastr": "~14.1.4",
    "ngx-trim-directive": "^1.1.0",
    "p-queue": "^6.6.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "webextension-polyfill": "^0.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.11",
    "@ngneat/svg-generator": "^3.0.1",
    "@types/d3": "^6.2.0",
    "@types/google.analytics": "^0.0.42",
    "@types/inputmask": "^5.0.1",
    "@types/jasmine": "~3.10.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/webextension-polyfill": "^0.8.1",
    "bestzip": "^2.1.7",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.3",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.6",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.11",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.12"
  },
  "svgGenerator": {
    "outputPath": "./shared/svg-icons",
    "postfix": "",
    "prefix": "svg",
    "srcPath": "./assets/svg",
    "svgoConfig": {
      "plugins": [
        "cleanupAttrs",
        "removeDimensions"
      ]
    }
  }
}
