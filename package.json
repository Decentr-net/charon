{
  "name": "charon",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "build:chrome": "npm run clean && npm run build:charon && npm run build:scripts:chrome && npm run build:toolbar",
    "build:chrome:prod": "npm run clean && npm run build:charon:prod && npm run build:scripts:chrome:prod && npm run build:toolbar:prod && npm run pack",
    "build:firefox": "npm run clean && npm run build:charon && npm run build:scripts:firefox && npm run build:toolbar",
    "build:firefox:prod": "npm run clean && npm run build:charon:prod && npm run build:scripts:firefox:prod && npm run build:toolbar:prod && npm run pack",
    "build:charon": "ng build charon",
    "build:charon:prod": "ng build charon --prod",
    "build:scripts": "webpack --config projects/scripts/webpack.config.dev.js",
    "build:scripts:prod": "webpack --config projects/scripts/webpack.config.prod.js",
    "build:scripts:chrome": "BROWSER=chrome npm run build:scripts",
    "build:scripts:chrome:prod": "BROWSER=chrome npm run build:scripts:prod",
    "build:scripts:firefox": "BROWSER=firefox npm run build:scripts",
    "build:scripts:firefox:prod": "BROWSER=firefox npm run build:scripts:prod",
    "build:toolbar": "ng build toolbar",
    "build:toolbar:prod": "ng build toolbar --prod",
    "generate-icons": "svg-to-ts",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "pack": "cd dist && bestzip ../extension-build.zip *",
    "test": "ng test",
    "watch:chrome": "npm run clean && npm-run-all --parallel watch:charon watch:scripts:chrome watch:toolbar",
    "watch:firefox": "npm run clean && npm-run-all --parallel watch:charon watch:scripts:firefox watch:toolbar",
    "watch:charon": "ng build charon --watch --delete-output-path=false --sourceMap=false",
    "watch:scripts": "webpack --config projects/scripts/webpack.config.dev.js -w",
    "watch:scripts:chrome": "BROWSER=chrome npm run watch:scripts",
    "watch:scripts:firefox": "BROWSER=firefox npm run watch:scripts",
    "watch:toolbar": "ng build toolbar --watch --delete-output-path=false --sourceMap=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@ngneat/reactive-forms": "^1.4.3",
    "@ngneat/svg-icon": "^2.1.0",
    "@ngneat/transloco": "^2.19.1",
    "@ngneat/until-destroy": "^8.0.3",
    "@rxweb/reactive-form-validators": "^2.1.2",
    "clamp-js": "^0.7.0",
    "d3": "~6.0.0",
    "decentr-js": "^1.7.0",
    "http-status-codes": "^2.1.4",
    "js-sha256": "^0.9.0",
    "ng-inline-svg": "^11.0.0",
    "ngx-quill": "^12.0.1",
    "ngx-toastr": "~12.1.0",
    "ngx-trim-directive": "^1.1.0",
    "p-queue": "^6.6.2",
    "quill": "^1.3.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/d3": "^6.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/resize-observer-browser": "^0.1.4",
    "bestzip": "^2.1.7",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "svg-to-ts": "^5.0.0",
    "ts-loader": "^8.0.11",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webextension-polyfill-ts": "^0.21.0",
    "webpack-cli": "^3.3.12"
  }
}
