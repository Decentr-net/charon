{
  "name": "charon",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:only": "ng serve -c develop",
    "build:angular": "ng build",
    "build:angular:prod": "ng build --prod",
    "watch:angular": "ng build --watch --delete-output-path false --sourceMap false",
    "build:chrome": "webpack --config chrome/webpack.config.js",
    "build:chrome:prod": "webpack -p --config chrome/webpack.config.prod.js",
    "watch:chrome": "webpack --config chrome/webpack.config.js -w",
    "build": "npm run clean && npm run build:angular && npm run build:chrome",
    "build:prod": "npm run build:angular:prod && npm run build:chrome:prod && npm run pack",
    "watch": "npm run clean && npm-run-all --parallel watch:*",
    "clean": "rimraf dist",
    "pack": "cd charon/dist && bestzip ../../extension-build.zip *",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@ngneat/reactive-forms": "^1.3.1",
    "@ngneat/transloco": "^2.19.1",
    "@ngneat/until-destroy": "^8.0.2",
    "d3": "~6.0.0",
    "decentr-js": "^1.1.15",
    "js-sha256": "^0.9.0",
    "ng-inline-svg": "^11.0.0",
    "p-queue": "^6.6.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@types/chrome": "0.0.123",
    "@types/firefox-webext-browser": "^78.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "awesome-typescript-loader": "^5.2.1",
    "bestzip": "^2.1.5",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.12"
  }
}
